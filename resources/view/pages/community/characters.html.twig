<script>
$(function(){
	var form = $("#searchForm");
	var input = $("#name");
	input.keyup(function(e){
		e.preventDefault();
		$.ajax({
			url: '{{URL}}/api/v1/searchcharacters',
			type: 'post',
			data: form.serialize(),
			dataType: "json",
			success: function(data){
				$(".searchResult").empty();
				$(data).each(function(key, value){
					$(".searchResult").append("<tr><td><img src='" + value.outfit.image_url + "'></td><td><a href='{{URL}}/community/characters/" + value.name + "'>" + value.name + "</a></td><td>" + value.level + "</td><td>" + value.vocation + "</td></tr>");
				});
			},
			error: function(data){
				$(".searchResult").text("Nenhum resultado encontrado.");
			},
		});
	});

});
</script>
